{
  "name": "wordpress-theme-starter",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:gersonlimadev/wordpress-theme-starter.git",
  "author": "Gerson Lima <gersonthiago.lima@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development webpack --watch --mode development --config config/webpack.config.js",
    "build": "NODE_ENV=production webpack --mode production --config config/webpack.config.js",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.scss": [
      "stylelint --syntax=scss",
      "git add"
    ],
    "src/**/*.js": [
      "eslint",
      "git add"
    ],
    "src/**/*.jsx": [
      "eslint",
      "git add"
    ]
  },
  "pre-commit": "lint-staged",
  "browserslist": [
    "last 2 versions",
    ">1%"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.12.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-prettier": "^3.0.1",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.2.1",
    "svg-url-loader": "^2.3.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-livereload-plugin": "^2.2.0"
  },
  "dependencies": {}
}
